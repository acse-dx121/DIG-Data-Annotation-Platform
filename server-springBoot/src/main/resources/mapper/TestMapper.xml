<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.uestc.labelproject.dao.TestMapper">

    <select id="get" resultType="com.uestc.labelproject.entity.Test">
        SELECT * FROM test
    </select>

    <sql id="insertKey">
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="test != null">
                test,
            </if>
            <if test="testLong != null">
                testLong,
            </if>
        </trim>
    </sql>
    <sql id="insertValue">
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="test != null">
                #{test},
            </if>
            <if test="testLong!=null">
                #{testLong},
            </if>
        </trim>
    </sql>
    <insert id="addTest" parameterType="com.uestc.labelproject.entity.Test">
        <!--insert into test <include refid="insertKey"/> <include refid="insertValue"/>-->
        insert into test(test,testLong) values (#{test}, #{testLong})
    </insert>
</mapper>